# importing random librabry to use random numbers 
import random 
print("Generate password that contains capital lettes, small letters, numbers and symbols ")

# give symbols
part_1=chr(random.randint(33,47+1))
# give capital letter
part_2=chr(random.randint(65,90+1))+chr(random.randint(65,90+1))
# give small Letter
part_3=chr(random.randint(97,122+1))+chr(random.randint(97,122+1))
# give numbers
part_4=str(random.randint(0,100))

# combining all the parts of password
password=part_1 + part_2 + part_3 + part_4
print()
print("password is : ",password)
